[app]
mode = production | development
host = 0.0.0.0
port = 80

[backend]
data_path = data/
data_features_name = features.pickle
im_indices_name = im_indicies.pickle
sessions_path = session_data/
results_path = results/
default_nbr_of_steps = 4
default_nbr_of_images = 30
labels = cat,dog

[api]
base_url = /api/v1

[frontend]
frontend_base = 
image_base = /
image_directory = images/
step_duration = 5
subsession_pause_duration = 0

[aws]
bucket = bucket-name
region = eu-north-1
access_key = <your access key>
secret_access_key = <your secret key>